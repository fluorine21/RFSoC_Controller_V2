
#ifndef _DMA_H_
#define _DMA_H_


//Function definitions////////////////////////

//Retuns 0 on success
u8 dma_init();
//Returns 0 on success
u8 dma_write_word(u32 word);
//Returns 0 on success, word returned through argument pointer
u8 dma_read_word(u32* dma_ret_val);

//////////////////////////////////////////////





#endif
